        "eslint-import-resolver-typescript": "^4.4.4",
        "eslint-plugin-import": "^2.32.0",
        "eslint-plugin-prettier": "^5.5.4",
        "eslint-plugin-react-hooks": "^5.2.0",
        "eslint-plugin-react-refresh": "^0.4.24",
        "globals": "^16.5.0",
        "husky": "^9.1.7",
        "jsdom": "^27.1.0",
        "lint-staged": "^16.2.6",
        "postcss": "^8.4.49",
        "prettier": "^3.6.2",
        "rollup-plugin-visualizer": "^6.0.5",
        "tailwindcss": "^3.4.13",

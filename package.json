        "@radix-ui/react-slot": "^1.1.0",
        "@tanstack/react-query": "^5.59.15",
        "class-variance-authority": "^0.7.1",
        "clsx": "^2.1.1",
        "exceljs": "^4.4.0",
        "fast-xml-parser": "^5.3.2",
        "file-saver": "^2.0.5",
        "firebase": "^12.6.0",
        "jschardet": "^3.1.4",
        "lucide-react": "^0.553.0",
        "papaparse": "^5.5.3",
        "pdfjs-dist": "^5.4.394",
        "react": "^19.2.0",
        "react-dom": "^19.2.0",
        "react-dropzone": "^14.3.8",
        "react-hook-form": "^7.66.0",
        "react-router-dom": "^7.9.5",
